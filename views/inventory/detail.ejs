<%- include('../layouts/header') %>

    <div class="container">
        <h1>
            <%= title %>
        </h1>

        <% if (messages && messages.notice) { %>
            <div class="alert alert-info">
                <%= messages.notice %>
            </div>
            <% } %>

                <% if (messages && messages.error) { %>
                    <div class="alert alert-danger">
                        <%= messages.error %>
                    </div>
                    <% } %>

                        <div class="vehicle-detail-container">
                            <%- detailHtml %>
                        </div>
    </div>

    <%- include('../layouts/footer') %>